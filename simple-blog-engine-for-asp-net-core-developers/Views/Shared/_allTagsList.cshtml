@model List<BlogPostTag>
<div class="tags-list">
    <div class="blog-widget">
        <h6 class="mb-20">Etiketler</h6>
        <ul class="list">
            @foreach (var tag in Model)
            {
                <li><a href="~/tag/@tag.Tag_URLFriendly">@tag.Tag (@tag.Count)</a></li>
            }
        </ul>

    </div>
</div>