@model List<BlogPostCategory>
<div class="tags-list">
    <div class="blog-widget">
        <h6 class="mb-20">Kategoriler</h6>
        <ul class="list">
            @foreach (var category in Model)
            {
                <li><a href="~/category/@category.Category_URLFriendly">@category.Category (@category.Count)</a></li>
            }
        </ul>

    </div>
</div>